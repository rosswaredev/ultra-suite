{
  "name": "ultra-suite",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npx expo start --dev-client",
    "android": "expo start --android",
    "ios": "npx expo start --dev-client --ios",
    "ios:install": "eas build:run -p ios --latest",
    "web": "expo start --web",
    "test": "vitest",
    "tokens": "yarn tokens:transform && yarn tokens:colors && yarn tokens:build",
    "tokens:transform": "token-transformer ./src/theme/figma-tokens.json src/theme/tokens.resolved.json global,semantic,components",
    "tokens:colors": "node ./scripts/colors.js",
    "tokens:build": "style-dictionary build --config sd.config.js",
    "typegen": "yarn typegen:pocket-base",
    "typegen:pocket-base": "pocketbase-typegen -d ./db/pocket-base/pb_data/data.db -o ./src/features/sync/pocket-base/types.ts",
    "pb:serve": "./db/pocket-base/pocketbase serve",
    "pb:clean": "node ./scripts/clean-pocket-base.js",
    "styles": "yarn styles:build && yarn styles:fix",
    "styles:prepare": "./scripts/prepare-styles.sh",
    "styles:build": "yarn tailwindcss -i ./src/theme/tailwind-input.css -o ./app/styles.css",
    "styles:fix": "./scripts/fix-styles.sh",
    "setup": "yarn typegen && yarn tokens && yarn styles",
    "build": "yarn build:preview:ios",
    "build:development:ios": "eas build --profile development --platform ios",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:sim:ios": "eas build --profile development-simulator --platform ios",
    "update": "yarn update:preview:ios",
    "update:preview:ios": "eas update --branch preview --platform ios --non-interactive --message",
    "ui": "yarn ladle serve",
    "icon": "npx remotion still ./app-icon/index.ts app-icon ./assets/icon.png",
    "icon:preview": "npx remotion preview ./app-icon/index.ts",
    "maestro:install": "curl -Ls \"https://get.maestro.mobile.dev\" | bash",
    "e2e": "./scripts/run-test.sh"
  },
  "dependencies": {
    "@expo/config-plugins": "~6.0.0",
    "@expo/vector-icons": "^13.0.0",
    "@gorhom/bottom-sheet": "^4",
    "@ladle/react": "^2.11.0",
    "@react-native-community/datetimepicker": "6.7.3",
    "@react-navigation/native": "^6.1.6",
    "@remotion/cli": "^3.3.83",
    "@types/react": "~18.0.27",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "expo": "^48.0.4",
    "expo-blur": "~12.2.2",
    "expo-build-properties": "~0.6.0",
    "expo-calendar": "~11.1.1",
    "expo-constants": "~14.2.1",
    "expo-crypto": "~12.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-device": "~5.2.1",
    "expo-haptics": "~12.2.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-linking": "~4.0.1",
    "expo-notifications": "~0.18.1",
    "expo-router": "^1.5.3",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.16.4",
    "lucide-react": "^0.139.0",
    "lucide-react-native": "^0.138.0",
    "mobx": "^6.8.0",
    "mobx-keystone": "^1.4.1",
    "mobx-react": "^7.6.0",
    "moti": "^0.25.1",
    "nanoid": "^4.0.1",
    "npm": "^9.6.4",
    "pocketbase": "^0.12.1",
    "pocketbase-ts": "^0.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.6",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-ios-context-menu": "^1.15.3",
    "react-native-is-maestro": "^2.0.0",
    "react-native-keyboard-controller": "^1.5.1",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-skeleton-content": "^1.0.28",
    "react-native-sse": "^1.1.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.18.7",
    "remotion": "^3.3.83",
    "twrnc": "^3.0.2-beta.1",
    "typescript": "^4.9.4",
    "vite": "^4.2.1",
    "vitest": "^0.29.7",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@hyrious/esbuild-plugin-commonjs": "^0.2.2",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/react-native": "^0.71.3",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "pocketbase-typegen": "^1.1.7",
    "postcss": "^8.4.21",
    "style-dictionary": "^3.7.2",
    "tailwindcss": "^3.2.7",
    "token-transformer": "^0.0.30",
    "vite-plugin-commonjs": "^0.6.2"
  },
  "resolutions": {
    "metro": "^0.73.7",
    "metro-resolver": "^0.73.7"
  },
  "overrides": {
    "metro": "^0.73.7",
    "metro-resolver": "^0.73.7"
  }
}
