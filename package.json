    "@types/react": "~18.0.27",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "expo": "^48.0.4",
    "expo-calendar": "~11.1.1",
    "expo-constants": "~14.2.1",
    "expo-crypto": "~12.2.1",
    "expo-haptics": "~12.2.1",
    "expo-linking": "~4.0.1",
    "expo-router": "^1.3.0",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.2",
    "mobx": "^6.8.0",
    "mobx-keystone": "^1.4.1",
    "mobx-react": "^7.6.0",
    "nanoid": "^4.0.1",
    "nativewind": "^2.0.11",
    "pocketbase": "^0.12.1",
    "pocketbase-ts": "^0.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.4",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-get-random-values": "^1.8.0",
